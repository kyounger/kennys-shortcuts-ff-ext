{
  "manifest_version": 2,
  "name": "Kenny's Shortcuts",
  "version": "0.6.0",
  "description": "Kenny's stuff",
  "browser_specific_settings": {
    "gecko": {
      "id": "kennysext@sheerfocus.com"
    }
  },
  "permissions": [
    "tabs",
    "activeTab",
    "contextualIdentities",
    "cookies"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "commands": {
    "kennys-shortcuts-1": {
      "suggested_key": {
        "default": "Ctrl+F1",
        "mac": "MacCtrl+F1"
      },
      "description": "Re-open in Container: Personal"
    },
    "kennys-shortcuts-2": {
      "suggested_key": {
        "default": "Ctrl+F2",
        "mac": "MacCtrl+F2"
      },
      "description": "Re-open in Container: Work"
    }
  }
}
